#, c-format
msgid "Volume %d%c (muted)"
msgstr "Lautstärke %d%c (Stumm)"

#: ../panel-plugin/pulseaudio-notify.c:178
#, c-format
msgid "Volume %d%c"
msgstr "Lautstärke %d%c"
